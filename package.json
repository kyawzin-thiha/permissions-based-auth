{
	"name": "permissions-based-auth",
	"version": "1.0.0",
	"description": "This is a simple NestJS application that demonstrates how to implement permissions-based authorization system.",
	"author": "Kyaw Zin Thiha",
	"private": false,
	"license": "MIT",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"build:prisma": "npx prisma db push && npx prisma db seed && npx prisma generate",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"prepare": "husky install"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.272.0",
		"@dicebear/avatars": "^4.10.8",
		"@dicebear/big-ears-neutral": "^4.10.8",
		"@nestjs/common": "^9.0.0",
		"@nestjs/core": "^9.0.0",
		"@nestjs/jwt": "^10.0.2",
		"@nestjs/platform-express": "^9.0.0",
		"@prisma/client": "^4.10.1",
		"@sendgrid/mail": "^7.7.0",
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"helmet": "^6.0.1",
		"redis": "^4.6.4",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.0.0",
		"@nestjs/schematics": "^9.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/express": "^4.17.13",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.0.0",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.2",
		"prettier": "^2.3.2",
		"prisma": "^4.10.1",
		"source-map-support": "^0.5.20",
		"ts-jest": "28.0.8",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.1.0",
		"typescript": "^4.7.4"
	},
	"lint-staged": {
		"*.ts": "eslint --cache --fix"
	}
}
